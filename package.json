{
  "name": "axette",
  "description": "Very simple and lightweight AJAX implementation for Nette",
  "version": "2.0.5",
  "main": "./dist/axette.cjs",
  "module": "./dist/axette.mjs",
  "exports": {
    ".": {
      "require": "./dist/axette.cjs",
      "import": "./dist/axette.mjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@vitest/coverage-c8": "^0.30.1",
    "dts-bundle-generator": "^8.0.1",
    "jsdom": "^21.1.1",
    "typescript": "^5.0.4",
    "vite": "^4.0.4",
    "vitest": "^0.31.4"
  },
  "maintainers": [
    "Matronator"
  ],
  "contributors": [
    "Matronator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matronator/axette.git"
  },
  "keywords": [
    "nette",
    "ajax",
    "axette",
    "lightweight"
  ],
  "author": "Matronator",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matronator/axette/issues"
  },
  "homepage": "https://github.com/matronator/axette#readme"
}
